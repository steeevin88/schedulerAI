generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model Users {
  email       String   @unique
  password    String
  icsUrl      String
  preferences String
  friends     String[]
}

model Events {
  id          Int      @id @default(sequence())
  email       String   
  event_name  String
  start_time  DateTime
  end_time    DateTime
}
